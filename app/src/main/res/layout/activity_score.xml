    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore9"
        android:layout_below="@+id/txtScore8"
        android:layout_toEndOf="@+id/lblPar9"
        android:layout_alignEnd="@+id/txtScore8"
        android:gravity="center" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnClear"
        android:id="@+id/btnClear"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblRoundScore"
        android:layout_below="@+id/txtScore1"
        android:layout_alignParentEnd="true"
        android:text="@string/lblRoundScore"
