<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".ScoreActivity"
    android:background="@drawable/golf_backdrop">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtName"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:text="@string/txtName"
        android:singleLine="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblHole"
        android:layout_below="@+id/txtName"
        android:layout_alignParentStart="true"
        android:text="@string/lblHole"
        android:inputType="none"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblPar"
        android:layout_below="@+id/txtName"
        android:layout_toEndOf="@+id/lblHole"
        android:text="@string/lblPar"
        android:inputType="none"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblScore"
        android:layout_below="@+id/txtName"
        android:layout_toEndOf="@+id/lblPar"
        android:text="@string/lblScore"
        android:inputType="none"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/lblHole1"
        android:layout_below="@+id/lblHole"
        android:text="@string/lblHole1"
        android:layout_alignEnd="@+id/lblHole"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:textColor="#ff000000"
        android:enabled="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/lblPar1"
        android:layout_below="@+id/lblPar"
        android:text="@string/lblPar1"
        android:layout_alignStart="@+id/lblPar"
        android:layout_alignEnd="@+id/lblPar"
        android:textColor="#ff000000"
        android:gravity="center"
        android:enabled="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore1"
        android:layout_below="@+id/lblScore"
        android:layout_alignStart="@+id/lblScore"
        android:layout_alignEnd="@+id/lblScore"
        android:gravity="center" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnIn"
        android:id="@+id/btnIn"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:visibility="invisible" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnTally"
        android:id="@+id/btnTally"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:enabled="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole2"
        android:layout_below="@+id/lblHole1"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole1"
        android:text="@string/lblHole2"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole3"
