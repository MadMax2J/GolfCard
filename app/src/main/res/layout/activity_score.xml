        android:id="@+id/txtScore5"
        android:layout_below="@+id/txtScore4"
        android:layout_toEndOf="@+id/lblPar5"
        android:layout_alignEnd="@+id/txtScore4"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore6"
        android:layout_below="@+id/txtScore5"
        android:layout_toEndOf="@+id/lblPar6"
        android:layout_alignEnd="@+id/txtScore5"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore7"
        android:layout_below="@+id/txtScore6"
        android:layout_toEndOf="@+id/lblPar7"
        android:layout_alignEnd="@+id/txtScore6"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore8"
        android:layout_below="@+id/txtScore7"
        android:layout_toEndOf="@+id/lblPar8"
        android:layout_alignEnd="@+id/txtScore7"
