        android:layout_alignEnd="@+id/lblPar8"
        android:text="@string/lblPar9"
        android:enabled="false"
        android:gravity="center"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore2"
        android:layout_below="@+id/txtScore1"
        android:layout_toEndOf="@+id/lblPar2"
        android:layout_alignEnd="@+id/txtScore1"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore3"
        android:layout_below="@+id/txtScore2"
        android:layout_toEndOf="@+id/lblPar3"
        android:layout_alignEnd="@+id/txtScore2"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtScore4"
        android:layout_below="@+id/txtScore3"
        android:layout_toEndOf="@+id/lblPar4"
        android:layout_alignEnd="@+id/txtScore3"
