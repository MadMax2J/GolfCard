    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/txtRoundScore"
        android:layout_below="@+id/lblRoundScore"
        android:layout_alignStart="@+id/lblRoundScore"
