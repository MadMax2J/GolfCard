        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole4"
        android:layout_below="@+id/lblHole3"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole3"
        android:text="@string/lblHole4"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole5"
        android:layout_below="@+id/lblHole4"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole4"
        android:text="@string/lblHole5"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole6"
        android:layout_below="@+id/lblHole5"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole5"
        android:text="@string/lblHole6"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole7"
        android:layout_below="@+id/lblHole6"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole6"
        android:text="@string/lblHole7"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole8"
        android:layout_below="@+id/lblHole7"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole7"
        android:text="@string/lblHole8"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblHole9"
        android:layout_below="@+id/lblHole8"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblHole8"
        android:text="@string/lblHole9"
        android:gravity="center"
        android:enabled="false"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblPar2"
        android:layout_below="@+id/lblPar1"
        android:layout_alignStart="@+id/lblPar1"
        android:layout_toStartOf="@+id/btnIn"
        android:layout_alignEnd="@+id/lblPar1"
        android:text="@string/lblPar2"
        android:enabled="false"
        android:gravity="center"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblPar3"
        android:layout_below="@+id/lblPar2"
        android:layout_alignStart="@+id/lblPar2"
        android:layout_alignEnd="@+id/lblPar2"
        android:text="@string/lblPar3"
        android:enabled="false"
        android:gravity="center"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblPar4"
        android:layout_below="@+id/lblPar3"
        android:layout_alignStart="@+id/lblPar3"
        android:layout_alignEnd="@+id/lblPar3"
        android:text="@string/lblPar4"
        android:enabled="false"
        android:gravity="center"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblPar5"
        android:layout_below="@+id/lblPar4"
        android:layout_alignStart="@+id/lblPar4"
        android:layout_alignEnd="@+id/lblPar4"
        android:text="@string/lblPar5"
        android:enabled="false"
        android:gravity="center"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/lblPar6"
        android:layout_below="@+id/lblPar5"
        android:layout_alignStart="@+id/lblPar5"
        android:layout_alignEnd="@+id/lblPar5"
        android:text="@string/lblPar6"
        android:enabled="false"
        android:gravity="center"
        android:textColor="#ff000000" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
